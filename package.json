		"@types/eslint": "8.56.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"smui-theme": "^7.0.0-beta.16",
		"svelte": "^4.2.7",
		"vite": "^5.0.3"
